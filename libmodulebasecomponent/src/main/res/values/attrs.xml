<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="DividerLine">
        <attr name="mode">
            <enum name="line" value="0" />
            <enum name="line_blank_line" value="1" />
            <enum name="line_blank" value="2" />
            <enum name="blank_line" value="3" />
            <enum name="blank" value="4"></enum>
        </attr>
        <attr name="lineColor" format="color|reference" />
    </declare-styleable>

    <declare-styleable name="ShSwitchView">
        <attr name="tintColor" format="reference|color" />
        <attr name="outerStrokeWidth" format="reference|dimension" />
        <attr name="shadowSpace" format="reference|dimension" />
    </declare-styleable>
</resources>